<div>
  <h1>Hola, {{user.name}}! Gusto verte! (texto desde handlebars)</h1>
  {{#if isAdmin}}
    {{#each food}}
      <div>
        <p>{{this.name}}</p>
        <p>{{this.price}}</p>
      </div>
    {{/each}}
  {{else}}
    <p>No tienes permisos para ver la lista de alimentos.</p>
  {{/if}}
</div>
<script src="/js/index.js"></script>